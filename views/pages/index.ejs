    <%- contentFor('hero')%>

        <script type="text/javascript">
            $(window).load(function () {
 


                function getBootstrapDeviceSize() {
                    return $('#users-device-size').find('div:visible').first().attr('id');
                }


                if (document.getElementById("overlay").className == 'false') {
                    document.getElementById("guestEyesOnly").style.display = "none";
                    document.getElementById("guestEyesOnly2").style.display = "none";
                    if ("<%= count%>" == "0") {
                        document.getElementById("hero").style.display = "none";
                        document.getElementById("overlay2").style = "display: block;background-color:rgba(130,126,124,0);  position: absolute;width:100%";
                    } else {
                        document.getElementById("hero").style.display = "block";
                        document.getElementById("overlay2").style.display = "none";

                    }


                } else if (document.getElementById("overlay").className == 'true') {
                    document.getElementById("guestEyesOnly").style.display = "inline";
                    document.getElementById("hero").style.display = "block";
                    document.getElementById("guestEyesOnly2").style.display = "block";
                    document.getElementById("mobile").style.display = "block";

                } else {

                    document.getElementById("hero").style.display = "none";
                    document.getElementById("mobile").style.display = "none";

                    if (getBootstrapDeviceSize() == ("xs") || getBootstrapDeviceSize() == ("sm") ||
                        getBootstrapDeviceSize() == ("md")) {
                        document.getElementById("overlay").style =
                            "display: block;  background-color:rgba(130,126,124, 0); position: absolute;";
                    } else {
                        document.getElementById("overlay").style = "display: block; background-color:rgba(130,126,124, 0); position: absolute;width:100%";

                    }

                }


                if (getBootstrapDeviceSize() == ("xs")) {
                    document.getElementById("nav").style = "display:none";
                    document.getElementById("intro").style = "font-size:40px";
                    document.getElementById("intro1").style = "font-size:40px";
                    document.getElementById("intro2").style = "font-size:40px";
                    document.getElementById("intro3").style = "font-size:40px";
                    document.getElementById("intro4").style = "font-size:40px";
                    document.getElementById("intro5").style = "font-size:40px";
                    document.getElementById("headerinfo").style = "width: 100%";
                    document.getElementById("headerh2").style = "width: 100%";
                    document.getElementById("body").style.height = "1000px";


                } else if (getBootstrapDeviceSize() == ("sm")) {
                    document.getElementById("nav").style = "display:none";
                    document.getElementById("intro").style = "font-size:45px";
                    document.getElementById("intro1").style = "font-size:45px";
                    document.getElementById("intro2").style = "font-size:45px";
                    document.getElementById("intro3").style = "font-size:45px";
                    document.getElementById("intro4").style = "font-size:45px";
                    document.getElementById("intro5").style = "font-size:45px";
                    document.getElementById("headerinfo").style = "width: 100%";
                    document.getElementById("headerh2").style = "width: 100%";
                    document.getElementById("body").style.height = "1200px";


                } else if (getBootstrapDeviceSize() == ("md")) {
                    document.getElementById("nav").style = "display:none";
                    document.getElementById("intro").style = "font-size:50px";
                    document.getElementById("intro1").style = "font-size:50px";
                    document.getElementById("intro2").style = "font-size:50px";
                    document.getElementById("intro3").style = "font-size:50px";
                    document.getElementById("intro4").style = "font-size:50px";
                    document.getElementById("intro5").style = "font-size:50px";
                    document.getElementById("headerinfo").style = "width: 100%";
                    document.getElementById("headerh2").style = "width: 100%";
                    document.getElementById("body").style.height = "14002px";

                } else {
                    document.getElementById("mobile").style.display = "none";
                    document.getElementById("nav").style = "display:block";
                    document.getElementById("intro").style = "font-size:100px";
                    document.getElementById("intro1").style = "font-size:100px";
                    document.getElementById("intro2").style = "font-size:100px";
                    document.getElementById("intro3").style = "font-size:100px";
                    document.getElementById("intro4").style = "font-size:100px";
                    document.getElementById("intro5").style = "font-size:100px";
                    document.getElementById("login").className = "form-elegant";
                    document.getElementById("login").style.margin = "20% 25%";
                    document.getElementById("login2").className = "form-elegant";
                    document.getElementById("login2").style.margin = "20% 25%";
                }

            });

            $('#navBar').mouseenter(function () {
                document.getElementById("loginFirst").style.display = "inline";
            })

            $('#navBar').mouseleave(function () {
                document.getElementById("loginFirst").style.display = "none";
            })

            //for mobile login in, move screen up
            var $htmlOrBody = $('html, body'),
                scrollTopPadding = 8;
            if (getBootstrapDeviceSize() == ("xs") || getBootstrapDeviceSize() == ("sm") || getBootstrapDeviceSize() ==
                ("md")) {
                $('.form-control').focus(function () {
                    var textareaTop = $(this).offset().top;
                    $htmlOrBody.scrollTop(textareaTop - scrollTopPadding);
                });
            }

            function on3() {

                getBootstrapDeviceSize();
                document.getElementById("overlay").style =
                    "display: block;background-color:rgba(130,126,124, 0);  position: absolute; height:100% width:100%";
                document.getElementById("overlay2").style.display = "none";

            }

            $("#btn2").mouseover(function () {

                if (document.getElementById("fnl").innerText() == null) {
                    window.alert("All feilds are required. Please fill out form before submitting");
                }
            })
        </script>


        <%- contentFor('body')%>
            <div id="users-device-size">
                <div id="xs" class="visible-xs"></div>
                <div id="sm" class="visible-sm"></div>
                <div id="md" class="visible-md"></div>
