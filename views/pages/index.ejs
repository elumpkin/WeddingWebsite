                                <div class='col-md-6 col-md-offset-3'>
                                    <form id="formLogin" action="/" method="POST">
                                        <div class="md-form">
                                            <input  style="font-size: 20px; " type="text" name="firstNameLogin" class="form-control" id="fnl" validate>
                                            <label style="font-size: 12px; " for="fnl" data-error="wrong" data-success="right">First Name</label>

                                        </div>
                                        <div class="md-form">

                                            <input  style="font-size: 20px; " type="text" name="lastNameLogin" class="form-control" id="lnl" validate>
                                            <label  style="font-size: 12px; " for="lnl" data-error="wrong" data-success="right">Last Name</label>
                                        </div>
                                        <div class="md-form">
                                            <input style="font-size: 20px; "  type="text"  name="password" class="form-control" id="pw" validate>
                                            <label  style="font-size: 12px; " data-error="wrong" data-success="right" for="pw">Password</label>
                                        </div>
                                        </br>
                                        <div class="md-form text-center">
