        <script type="text/javascript">
            $(window).load(function () {
                ////window.alert("<%= guest%>");

              


                console.log("<%= guest%>");
                console.log(document.getElementById("loginFirst").className);
               

                if ("<%= guest%>" == 'false') {
                    // //window.alert(document.getElementById("overlay").className);
                    //  document.getElementById("loginFirst").style.display = "inline";
                    document.getElementById("guestEyesOnly").style.display = "none";
                    ////window.alert("<%= guest%>");

                } else if ("<%= guest%>" == 'none') {
                    document.getElementById("loginFirst").style.display = "none";
                    document.getElementById("guestEyesOnly").style.display = "none";
                    ////window.alert("<%= guest%>");

                } else {
                    // document.getElementById("loginFirst").style.display = "block";
                    document.getElementById("guestEyesOnly").style.display = "inline";
                    document.getElementById("guestEyesOnly2").style.display = "block";

                    ////window.alert("<%= guest%>");
                }

                function getBootstrapDeviceSize() {
                    return $('#users-device-size').find('div:visible').first().attr('id');
                    }
                    if (getBootstrapDeviceSize() == ("xs")){
                        document.getElementById("mobile").style = "display:block";
                        document.getElementById("nav").style = "display:none";
                 
                    }else if(getBootstrapDeviceSize() == ("sm")){
                        document.getElementById("mobile").style = "display:block";
                        document.getElementById("nav").style = "display:none";

                    }else if(getBootstrapDeviceSize() == ("md")){
                        document.getElementById("mobile").style = "display:block";
                        document.getElementById("nav").style = "display:none";

                    }else{
                        document.getElementById("mobile").style = "display:none";
                        document.getElementById("nav").style = "display:block";

                    }


            });
        </script>
