 <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>
 <script  type="text/javascript">
 $(window).load(function()
{
  document.getElementById('body').id = 'index';
  function getBootstrapDeviceSize() {
    return $('#users-device-size').find('div:visible').first().attr('id');
    }
    if (getBootstrapDeviceSize() == ("xs")){
        document.getElementById("mobile").style = "display:block";
        document.getElementById("nav").style = "display:none";
        //window.alert("xs");
    }else if(getBootstrapDeviceSize() == ("sm")){
        document.getElementById("mobile").style = "display:block";
        document.getElementById("nav").style = "display:none";
        //window.alert("sm");

    }else if(getBootstrapDeviceSize() == ("md")){
        document.getElementById("mobile").style = "display:block";
        document.getElementById("nav").style = "display:none";
        //window.alert("md");

    }else{
        document.getElementById("mobile").style = "display:block";
        document.getElementById("nav").style = "display:block";
        //window.alert(getBootstrapDeviceSize());

    }
});

        $(window).scroll(function() {
 
          if ($(this).scrollTop()>0)
          {
             $('#headercontent').fadeOut();
             $('#headerh2').removeClass("h1").addClass("h3");
          }
         else
          {
           $('#headercontent').fadeIn();
           $('#headerh2').removeClass("h3").addClass("h1");
          }
  });
     </script>
 

